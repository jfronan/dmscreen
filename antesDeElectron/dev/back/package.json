{
  "name": "dmserver",
  "version": "1.0.0",
  "description": "Api de archivos de app DMscreen",
  "main": "index.js",
  "scripts": {
    "test": "test",
    "start": "nodemon index.js $INIT_CWD %INIT_CWD%",
    "installRun": "npm install && npm run createExec && nodemon index.js $INIT_CWD %INIT_CWD%",
    "createExec": "nexe -i index.js -o ../dmscreen -r '../front' -r '../data' -t 'windows-x86-6.1.0' && nexe -i index.js -o ../dmscreen -r '../front' -r '../data' -t 'mac-x64-10.12.0'"
  },
  "author": "Juan Francisco Ronanduano",
  "license": "MIT",
  "dependencies": {
    "body-parser": "^1.18.3",
    "express": "^4.16.4",
    "opn": "^5.5.0"
  },
  "devDependencies": {
    "nodemon": "^1.18.10"
  }
}
